{

    "main": {
        "sys.var.set": [{
            "id": "my_var",
            "value": 10
        }],

        "null": "@my_function",
		"@my_another_function" : "@my_var",
        "sys.out": {
            "0": "@my_another_function",
            "1": "End"

        }

    },
    "my_function": {
        "return": null
    },

    "my_another_function": {
        "return": "$input+1"
    }

}
